<p id="notice"><%= notice %></p>

<h1>Userprofiles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Dob</th>
      <th>Cid no</th>
      <th>Email</th>
      <th>Contact no</th>
      <th>Address</th>
      <th>Users</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @userprofiles.each do |userprofile| %>
      <tr>
        <td><%= userprofile.name %></td>
        <td><%= userprofile.dob %></td>
        <td><%= userprofile.cid_no %></td>
        <td><%= userprofile.email %></td>
        <td><%= userprofile.contact_no %></td>
        <td><%= userprofile.address %></td>
        <td><%= userprofile.users_id %></td>
        <td><%= link_to 'Show', userprofile %></td>
        <td><%= link_to 'Edit', edit_userprofile_path(userprofile) %></td>
        <td><%= link_to 'Destroy', userprofile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Userprofile', new_userprofile_path %>
